{
  "name": "@lmnr-ai/client",
  "version": "0.8.12",
  "description": "HTTP client for Laminar AI API",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "types": {
        "require": "./dist/index.d.cts",
        "import": "./dist/index.d.mts"
      },
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "types": "dist/index.d.cts",
  "scripts": {
    "prepare": "tsdown",
    "build": "tsdown",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "tsx --test test/*.test.ts"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lmnr-ai/lmnr-ts.git"
  },
  "keywords": [
    "laminar",
    "lmnr",
    "client"
  ],
  "author": "founders@lmnr.ai",
  "license": "Apache-2.0",
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "dotenv": "^17.2.2",
    "pino": "9.12.0",
    "pino-pretty": "^13.1.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@lmnr-ai/types": "workspace:*",
    "nock": "^14.0.8",
    "tsx": "^4.20.3"
  }
}